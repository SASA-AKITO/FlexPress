complete -c FlexPress -s m -l mode -d 'Mode of operation.' -r -f -a "{auto\t'',archive\t'',extract\t'',list\t''}"
complete -c FlexPress -s o -l output -d 'Output file in archive mode, or output directory in extraction mode' -r -F
complete -c FlexPress -l to-archive-name-dir -d 'extract files to DEST/ARCHIVE_NAME directory (extract mode).'
complete -c FlexPress -s n -l no-recursive -d 'No recursive directory (archive mode).'
complete -c FlexPress -s v -l verbose -d 'Display verbose output.'
complete -c FlexPress -l overwrite -d 'Overwrite existing files.'
complete -c FlexPress -s h -l help -d 'Print help'
complete -c FlexPress -s V -l version -d 'Print version'
